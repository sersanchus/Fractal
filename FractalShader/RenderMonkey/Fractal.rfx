   for (deep=1; deep<5; deep++)
   {
      f = texture2D(texORI,posaux).x;
           
      a = texture2D(texCOEF,vec2(0.0625,f)).x*4.0-2.0;
      b = texture2D(texCOEF,vec2(0.1875,f)).x*4.0-2.0;
      c = texture2D(texCOEF,vec2(0.3125,f)).x*4.0-2.0;
      d = texture2D(texCOEF,vec2(0.4375,f)).x*4.0-2.0;
      e = texture2D(texCOEF,vec2(0.5625,f)).x;
      f = texture2D(texCOEF,vec2(0.6875,f)).x;
          
      auxv=posaux-((vec2(e,f)+floor(posaux*transx))*invtransx); 
      posaux=(vec2(a*auxv.x+b*auxv.y,c*auxv.x+d*auxv.y)+texture2D(texDOMPOS,posaux).xy);
     
      auxv = texture2D(texSO,posaux).xy*2.0-1.0;
      factoracum += factorultimo;
      factorultimo = factor0*auxv.y;
      factor0 *= auxv.x;           
   } 
  
   gl_FragColor = vec4(semilla * factor0 + factoracum + factorultimo);
}
